  <div class="container"><span class="hidden" id="idpaciente" value=""></span>
    <div class="modal fade" id="modalContrasena" role="dialog">
      <div class="modal-dialog">
         <form class="form-horizontal" role="form" id="cambiarPassword">
        <!-- Modal content-->
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">Cambiar contrase&ntilde;a</h4>
          </div>
          <div class="modal-body">
            <div class="form-group">
              <label class="col-lg-4 control-label">Ingrese su contrase&ntilde;a: </label>
              <div class="col-lg-7">
                <input id="passwordold" name="passwordold" class="form-control" type="password" placeholder="Contrase&ntilde;a" required="true" autocomplete="off">
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-4 control-label">Nueva contrase&ntilde;a: </label>
              <div class="col-lg-7">
                <input id="password1" name="password1" class="form-control" type="password" placeholder="Contrase&ntilde;a" required="true" autocomplete="off">
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-4 control-label">Repita nueva contrase&ntilde;a: </label>
              <div class="col-lg-7">
                <input id="password2" name="password2" class="form-control" type="password" placeholder="Contrase&ntilde;a" required="true" autocomplete="off">
              </div>
            </div>
            <br>      
          </div>
            <div class="modal-footer">
              <button id="btnCambiarPassword" type="submit" class="btn btn-primary">Guardar</button>
              <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
          </form>
        </div>
      </div>
    </div>
    <!--START section bienvenido-->
    <div id="section1" class="container-fluid" >
       <h1 style="text-align: left;" id="pacienteNombre">Editar Perfil <i class="glyphicon glyphicon-pencil" data-toggle="collapse" data-target=".navbar-collapse.in" ></i></h1>
      <hr/>
      <div class="row">
          <!-- left column -->
         
          <!-- edit form column -->
          <div class="col-md-9 infoPerfil">
            <form id="perfilForm" class="form-horizontal" role="form">
              <div class="form-group">
                <label for="nombres" class="col-lg-3 control-label">Nombres:</label>
                <div class="col-lg-8">
                  <input id="nombres" name="nombres" class="form-control" type="text" placeholder="Ingrese su nombre">
                </div>
              </div>
              <div class="form-group">
                <label for="apellidos" class="col-lg-3 control-label">Apellidos:</label>
                <div class="col-lg-8">
                  <input id="apellidos" name="apellidos" class="form-control" type="text" placeholder="Ingrese su apellido">
                </div>
              </div>
              <div class="form-group">
                <label for="cedula" class="col-lg-3 control-label">C&eacute;dula:</label>
                <div class="col-lg-8">
                  <input id="cedula" name="cedula" class="form-control" type="text" placeholder="Ingrese su n&uacute;mero de c&eacute;dula">
                </div>
              </div>
              <div class="form-group">
                <label for="email" class="col-lg-3 control-label">E-mail:</label>
                <div class="col-lg-8">
                  <input id="email" name="email" class="form-control" type="email" placeholder="Ingrese su direcci&oacute;n de correo electr&oacute;nico">
                </div>
              </div>
              <div class="form-group">
                <label class="col-lg-3 control-label"></label>
                <div class="col-lg-8">
                  <input type="submit" class="btn btn-primary" value="Guardar">
                  <span></span>
                  <input type="button" onclick="location.href='/paciente';" class="btn btn-default" value="Cancelar">
                </div>
              </div>
            </form>
          </div>
      </div>
